    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.14.2",
    "skeleton-elements": "^4.0.1",
    "swiper": "^10.0.4",
    "zustand": "^4.3.9"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.0.3",
    "cpy-cli": "^5.0.0",
